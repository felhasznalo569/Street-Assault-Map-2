<!DOCTYPE html>
<html>
  <head>
    <title>The Best Places on Earth</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="container">
      </div>
      <div class="info">
        <h3>List all crimes</h3>
        <a href="/new">Create new crime</a>
        <ul>
          <% crimes.forEach((crime) => { %>
            <li>
              <a href="/<%= crime.id%>">
                <%= crime.name%><span><%= crime.description %></span>
              </a>
               |
              <a href="/<%= crime.id%>/edit">Edit</a>
               |
              <a href="/<%= crime.id%>/delete">Delete</a>
            </li>
          <% }) %>
        </ul>
      </div>
    </div>
    <div id="map"></div>
  </body>
  <script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCbBJPvYIaR5ozIx_0-uWiGeO9cK4IxHww">
  </script>
  <script type="text/javascript">
 var myCrimes = <%- JSON.stringify(crimes) %>;
 </script>
  <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
  <script type="text/javascript" src="/javascripts/main.js"></script>
</html>
